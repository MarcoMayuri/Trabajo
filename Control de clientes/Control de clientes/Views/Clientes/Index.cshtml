@model IEnumerable<Control_de_clientes.Identity.Cliente>

@{
    ViewBag.Title = "Index";
}

<h2>Registro de clientes</h2>

<p>
    @Html.ActionLink("Agregar registro", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VNOMBRE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VAPELLIDO_PATERNO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VAPELLIDO_MATERNO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VDIRECCION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VDNI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VTELEFONO)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VNOMBRE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VAPELLIDO_PATERNO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VAPELLIDO_MATERNO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VDIRECCION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VDNI)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VTELEFONO)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ICLIENTE_ID }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.ICLIENTE_ID }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.ICLIENTE_ID })
        </td>
    </tr>
}

</table>
